{
  "manifest_version" : 3,
  "name" : "Tabify", 
  "description": "This is a Chrome Extension for new tabs",
  "version" : "0.0.12",
  "chrome_url_overrides" : { "newtab" : "index.html" },
  "permissions" : [ "tabs","storage" ],
  "icons" : { "128" : "/assets/images/kia.png" },
  "options_page": "/assets/pages/options.html"
}